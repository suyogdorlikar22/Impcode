public class ItemObject {
    public String name;
    public int imageId;

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getImageId() {
        return imageId;
    }

    public void setImageId(int imageId) {
        this.imageId = imageId;
    }

    public ItemObject(String name, int imageId) {
        this.name = name;
        this.imageId = imageId;
    }
}
